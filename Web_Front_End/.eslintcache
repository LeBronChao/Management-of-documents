> {\n    DocListReq(DocListRender, doc_type)\n  }, [])\n\n\n  function DocListRender(list) {\n    for (let i = 0; i < list.length; i++) {\n      let title = [list[i].title, list[i].color, list[i].bold]\n      list[i].title = title\n      list[i].date = list[i].date.slice(0, 10)\n      delete list[i].color\n      delete list[i].bold\n    }\n    setDocList(list)\n    setTimeout(() => {\n      setLoading(false)\n    }, 500)\n\n  }\n\n  function GetDocList(e) {\n    setLoading(true)\n    setDocType(e.target.value)\n    DocListReq(DocListRender, e.target.value)\n  }\n\n  function SerachDoc(value) {\n    DocListReq(DocListRender, doc_type, value)\n    setLoading(true)\n  }\n\n  useEffect(() => {\n    setDocType(doc_type)\n  }, [doc_type])\n\n  useEffect(() => {\n    setDocList(doc_list)\n  }, [doc_list])\n\n\n  return (\n    <div className=\"sp\" style={{ width: \"1440px\" }}>\n      <div className=\"doclist-container\">\n        <div className=\"doclist-header row\">\n          <nav className=\"doclist-header-nav row\">\n            <Radio.Group value={doc_type} onChange={GetDocList}>\n              <Radio.Button value=\"全部信息\">全部信息</Radio.Button>\n              <Radio.Button value=\"教学教务\">教学教务</Radio.Button>\n              <Radio.Button value=\"学术科研\">学术科研</Radio.Button>\n              <Radio.Button value=\"行政通知\">行政通知</Radio.Button>\n              <Radio.Button value=\"学生工作\">学生工作</Radio.Button>\n              <Radio.Button value=\"校园生活\">校园生活</Radio.Button>\n            </Radio.Group>\n          </nav>\n          <Search\n            placeholder=\"输入标题搜索\"\n            allowClear\n            onSearch={SerachDoc}\n            style={{ width: 360, margin: '0 0 0 196px ' }}\n          />\n        </div>\n        <main className=\"doclist-body\">\n          <Table dataSource={doc_list} columns={columns} loading={loading} pagination={{ pageSize: 12 }} onRow={record => {\n            return {\n              onClick: event => {\n                DocClickReq(record.doc_no)\n                props.history.push(`/Doc/Detail/${record.doc_no}`)\n              }, // 点击行\n            };\n          }} />\n        </main>\n      </div>\n    </div>\n  )\n}\n\nexport default DocList","/Users/lebronchao/Desktop/Web Project/Management of documents/Web_Front_End/src/reportWebVitals.js",[],["85","86"],"/Users/lebronchao/Desktop/Web Project/Management of documents/Web_Front_End/src/pages/Home/index.jsx",[],"/Users/lebronchao/Desktop/Web Project/Management of documents/Web_Front_End/src/api/DocList.js",[],"/Users/lebronchao/Desktop/Web Project/Management of documents/Web_Front_End/src/App.js",[],"/Users/lebronchao/Desktop/Web Project/Management of documents/Web_Front_End/src/api/Detail.js",[],["87","88"],"/Users/lebronchao/Desktop/Web Project/Management of documents/Web_Front_End/src/index.js",[],{"ruleId":"89","severity":1,"message":"90","line":13,"column":6,"nodeType":"91","endLine":13,"endColumn":8,"suggestions":"92"},{"ruleId":"93","severity":1,"message":"94","line":28,"column":62,"nodeType":"95","endLine":28,"endColumn":77},{"ruleId":"96","severity":1,"message":"97","line":42,"column":35,"nodeType":"98","messageId":"99","endLine":42,"endColumn":37},{"ruleId":"100","severity":1,"message":"101","line":56,"column":9,"nodeType":"102","messageId":"103","endLine":63,"endColumn":10},{"ruleId":"89","severity":1,"message":"104","line":77,"column":6,"nodeType":"91","endLine":77,"endColumn":8,"suggestions":"105"},{"ruleId":"89","severity":1,"message":"90","line":15,"column":6,"nodeType":"91","endLine":15,"endColumn":8,"suggestions":"106"},{"ruleId":"93","severity":1,"message":"94","line":34,"column":62,"nodeType":"95","endLine":34,"endColumn":77},{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"96","severity":1,"message":"97","line":44,"column":35,"nodeType":"98","messageId":"99","endLine":44,"endColumn":37},{"ruleId":"89","severity":1,"message":"104","line":75,"column":6,"nodeType":"91","endLine":75,"endColumn":8,"suggestio